{
  "expo": {
    "name": "Memories",
    "slug": "pensieri-app",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/logo_nuovo.png",
        "resizeMode": "contain",
        "backgroundColor": "#009485"
      }
    },
    "android": {
      "package": "it.codeitall.mobi.memoriae",
      "versionCode": 1,
      "splash": {
        "mdpi": "./assets/images/logo_m.png",
        "hdpi": "./assets/images/logo_h.png",
        "xhdpi": "./assets/images/logo_x.png",
        "xxhdpi": "./assets/images/logo_xx.png",
        "xxxhdpi": "./assets/images/logo_nuovo.png",
        "resizeMode": "contain",
        "backgroundColor": "#009485"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "description": ""
  }
}